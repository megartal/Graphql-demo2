type User {
    id: ID! @unique
    name: String!
    email: String! @unique
    password: String!
    links: [Link!]!
    votes: [Vote!]!
}

type Auth{
    token: String!
    user: User!
}

extend type Query{
    findAllUsers: [User!]!
}

extend type Mutation{
    signup(email: String!, password: String!, name: String!): Auth

    login(email: String!, password: String!): Auth
}