type User {
    id: ID! @unique
    name: String!
    email: String! @unique
    password: String!
    links: [Link!]!
    votes: [Vote!]!
}

type SigninPayload {
    token: String
    user: User
}

input AuthData {
    username: String!
    token: String!
}

extend type Query{
    findAllUsers: [User!]!
}

extend type Mutation{
    signup(auth: AuthData): SigninPayload
}